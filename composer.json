{
    "name": "xjaqil/laravel-microapp-sdk",
    "description": "Micro SDK for Laravel",
    "homepage": "https://github.com/xjaqil/laravel-microapp-sdk",
    "keywords": [
        "micro-pay",
        "sdk"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xjaqil",
            "email": "xjaqil@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Aqil\\MicroApp\\": "src/"
        },
        "files": [
            "src/Kernel/Support/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Aqil\\MicroApp\\ServiceProvider"
            ],
            "aliases": {
                "MicroApp": "\\Aqil\\MicroApp\\Facade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
